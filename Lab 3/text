# 1
count = 0
for i in range(1, 101):
    if str(i ** 2)[-1] == str(4):
        count += 1
print(count)

# 2
num = 0
for i in range(1, 6):
    x = int(input("Enter a number: "))
    if x > num:
        num = x

print(num)
# 3
from random import randint

rand_num = randint(1, 5)
for i in range(6):
    print('Hello' * rand_num)

for i in range(6):
    rand_num = randint(1, 5)
    print('Hello' * rand_num)

x = 0
while x < 11:
    print(x, " is less than 11.")
    x+=1
